{
  "manifest_version": 3,
  "name": "Auto Bookmark Tracker",
  "version": "1.0",
  "description": "Automatically updates bookmarks to track your progress on specific websites",
  "permissions": [
    "bookmarks",
    "storage",
    "tabs"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
